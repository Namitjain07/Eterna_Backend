{
  "name": "eterna_backend",
  "version": "1.0.0",
  "description": "ETERNA Order Execution Engine with DEX routing and WebSocket updates",
  "main": "dist/server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "db:init": "node -e \"require('./dist/db/database').database.initialize()\"",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Namitjain07/Eterna_Backend.git"
  },
  "keywords": [
    "solana",
    "dex",
    "trading",
    "order-execution",
    "websocket",
    "raydium",
    "meteora"
  ],
  "author": "Namit Jain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Namitjain07/Eterna_Backend/issues"
  },
  "homepage": "https://github.com/Namitjain07/Eterna_Backend#readme",
  "dependencies": {
    "@fastify/websocket": "^11.2.0",
    "bullmq": "^5.63.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "uuid": "^9.0.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "@types/pg": "^8.15.6",
    "@types/uuid": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  }
}
